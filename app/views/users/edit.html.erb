<%= @user.errors.full_messages.join(", ") %>
<div class = 'well bs-component'>
<legend>Edit Account Info <%= link_to "Forgot Password?", edit_password_path  %></legend>
<%= simple_form_for @user, url:users_path do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
  <%= f.button :submit, class: "btn btn-primary" do %>
    Save changes
  <% end %>
<% end %>
</div>
